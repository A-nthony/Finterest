
<div class="container p-4">
    <div class="grid">
      {{#each photos}}
        <div class="grid-item animate__animated animate__fadeInUp mb-2">
          <div class="card" style="width: 16rem;">
             <img src="{{imageURL}}" class="card-img-top" alt="image">
             <div class="card_body p-2">
               <h5 class="card-title">{{title}}</h5>
               <p class="card-text">{{description}}</p>
               <p class="card-text"></p>
               <a href="{{imageURL}}" class="btn btn-primary" target="_blank"><i class="far fa-eye"></i></a>
             </div>
          </div>
        </div>
      {{/each}}
    </div>
</div>